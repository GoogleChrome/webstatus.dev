{
  "name": "frontend",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.14.0"
  },
  "description": "webstatus.dev frontend",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run tsc-clean && tsc && npx rollup -c",
    "clean": "rm -rf build && rm -rf static && rm -rf dist && rm -rf node_modules && rm -rf .postinstall",
    "lint": "cd .. && prettier frontend/ --check && cd frontend && eslint \"**/*.{js,ts}\" && lit-analyzer \"src/**/*.{js,ts}\" && tsc --noEmit",
    "lint-fix": "cd .. && prettier frontend/ --write && cd frontend && eslint \"**/*.{js,ts}\" --fix",
    "local": "npm run build && TODO",
    "postinstall": "./scripts/postinstall.js",
    "start": "node dist/server/index.js",
    "test": "npm run tsc-clean && tsc && wtr --coverage --node-resolve --playwright --browsers chromium firefox webkit",
    "tsc-clean": "tsc --build --clean",
    "test:watch": "tsc && concurrently -k --raw \"tsc --watch --preserveWatchOutput\"  \"wtr --watch --coverage --node-resolve --playwright --browsers chromium firefox webkit\" "
  },
  "devDependencies": {
    "@browser-logos/chrome": "^2.0.0",
    "@browser-logos/chrome-canary": "^2.0.0",
    "@browser-logos/edge": "^2.0.7",
    "@browser-logos/edge-dev": "^2.0.3",
    "@browser-logos/firefox": "^3.0.10",
    "@browser-logos/firefox-nightly": "^3.0.6",
    "@browser-logos/safari": "^2.1.0",
    "@browser-logos/safari-technology-preview": "^2.0.2",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "@types/webstatus.dev-backend": "file:../lib/gen/openapi/ts-webstatus.dev-backend-types",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@web/dev-server-esbuild": "^1.0.1",
    "@web/rollup-plugin-html": "^2.0.1",
    "@web/test-runner": "^0.18.0",
    "@web/test-runner-mocha": "^0.9.0",
    "@web/test-runner-playwright": "^0.11.0",
    "chai": "^5.1.1",
    "cpy": "^11.0.1",
    "eslint": "^9.9.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-lit": "^1.10.1",
    "eslint-plugin-perfectionist": "^3.1.3",
    "eslint-plugin-promise": "^7.0.0",
    "lit-analyzer": "^2.0.2",
    "openapi-typescript-helpers": "^0.0.11",
    "playwright": "^1.45.0",
    "prettier": "3.3.3",
    "rollup": "^4.21.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.5.2",
    "sinon": "^18.0.0",
    "ts-lit-plugin": "^2.0.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@lit/context": "^1.1.0",
    "@lit/task": "^1.0.0",
    "@shoelace-style/shoelace": "^2.13.1",
    "@types/google.visualization": "^0.0.74",
    "@vaadin/router": "^1.7.5",
    "firebase": "^10.12.5",
    "lit": "^3.0.1",
    "openapi-fetch": "^0.10.5"
  }
}
